[user]
	name = inux39
	email = inux.0x27@gmail.com
[push]
	default = simple
[core]
	editor = vim
[help]
	autocorrect = 20
[merge]
	ff = false
[pull]
	ff = only
[alias]
	ls = log --graph
	pl = pull
	branches = branch -a
	tags = tag -l
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	cm = commit
	co = checkout

