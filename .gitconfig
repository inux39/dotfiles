[user]
	name = inux39
	email = inux39@protonmail.ch
	signingkey = A8DBCB6E6F1C6116782634E45CC1740973BD71B0
[push]
	default = simple
[core]
	editor = vim
[help]
	autocorrect = 20
[merge]
	ff = false
[pull]
	ff = only
[alias]
	ls = log --graph
	pl = pull
	branches = branch -a
	tags = tag -l
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	cm = commit
	co = checkout
	diffs = diff --word-diff-regex=. --word-diff=color

[color]
	ui = auto
